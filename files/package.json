{
    "private": true,
    "type": "module",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "vite",
        "dev:open": "vite --open",
        "build": "vite build",
        "type-check": "vue-tsc --noEmit",
        "test": "vitest",
        "test:ui": "vitest run --ui",
        "coverage": "vitest run --coverage",
        "version": "auto-changelog --template keepachangelog --hide-credit -p && git add CHANGELOG.md",
        "lint": "eslint --ext .js,.ts,.vue .",
        "lint:fix": "eslint --ext .js,.ts,.vue . --fix",
        "format": "prettier . --write",
        "commit": "git add --all && commit",
        "commit-push": "git add --all && commit && git push origin --all",
        "execute-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\" && npx husky add .husky/commit-msg \"npx --no -- commitlint --config commitlint.config.cjs --edit $1\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{js,ts,vue}": [
            "eslint . --fix",
            "prettier . --write"
        ]
    },
    "devDependencies": {
        "@commitlint/cli": "^17.8.1",
        "@commitlint/config-conventional": "^17.8.1",
        "@commitlint/prompt-cli": "^17.8.1",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/crypto-js": "^4.2.2",
        "@types/jsdom": "^21.1.6",
        "@types/leaflet": "^1.9.9",
        "@types/lodash": "^4.17.0",
        "@types/uuid": "^8.3.4",
        "@types/validator": "^13.11.9",
        "@types/vue-tel-input": "^2.1.6",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "@vitejs/plugin-vue": "^4.6.2",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/tsconfig": "^0.5.1",
        "auto-changelog": "^2.4.0",
        "autoprefixer": "^10.4.18",
        "eslint": "^8.57.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.24.0",
        "flowbite": "^2.3.0",
        "husky": "^8.0.3",
        "jsdom": "^24.0.0",
        "laravel-vite-plugin": "^1.0.1",
        "lint-staged": "^13.3.0",
        "naive-ui": "^2.38.1",
        "postcss": "^8.4.36",
        "prettier": "^2.8.8",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.4.4",
        "unplugin-auto-import": "^0.17.5",
        "unplugin-vue-components": "^0.26.0",
        "vfonts": "^0.0.3",
        "vite": "^5.1.4",
        "vitest": "^0.34.6",
        "vue-tsc": "^2.0.10"
    },
    "dependencies": {
        "@egjs/flicking-plugins": "^4.7.1",
        "@egjs/vue3-flicking": "^4.11.2",
        "@inertiajs/core": "^1.0.15",
        "@inertiajs/vue3": "^1.0.15",
        "@unhead/vue": "^1.9.10",
        "@vueuse/core": "^10.9.0",
        "axios": "^1.6.7",
        "browser-image-compression": "^2.0.2",
        "crypto-js": "^4.2.0",
        "jspdf": "^2.5.1",
        "laravel-vue-i18n": "^2.7.6",
        "leaflet": "^1.9.4",
        "libphonenumber-js": "^1.11.1",
        "localforage": "^1.10.0",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "pinia": "^2.1.7",
        "pinia-persistence-plugin": "^0.0.5",
        "slugify": "^1.6.6",
        "uuid": "^9.0.1",
        "validator": "^13.11.0",
        "vite-svg-loader": "^5.1.0",
        "vue": "^3.4.37",
        "vue-recaptcha": "3.0.0-alpha.6",
        "vue-socials": "^2.0.5",
        "vue-tel-input": "^8.3.1"
    }
}
